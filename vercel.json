{
  "buildCommand": "cd frontend && bun install && bun run build",
  "outputDirectory": "frontend/.next",
  "installCommand": "cd frontend && bun install",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://your-backend-url.com/api/:path*"
    }
  ],
  "functions": {
    "frontend/src/app/**/*.tsx": {
      "runtime": "nodejs20.x"
    }
  }
}